<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="277.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FxHistoryController">
   <children>
      <VBox prefHeight="271.0" prefWidth="365.0">
         <children>
            <TextArea fx:id="txtHistory" prefHeight="329.0" prefWidth="600.0" />
            <HBox prefHeight="77.0" prefWidth="365.0">
               <children>
                  <Button fx:id="btnHide" mnemonicParsing="false" onAction="#onclickHide" text="Hide">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button fx:id="btnClear" mnemonicParsing="false" onAction="#onclickClear" text="Clear">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
